<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">


<mapper namespace="goods">

	<select id="goodsList" resultType="com.javaex.vo.GoodsVo">
		
		<![CDATA[
			select	 no
        			,category
        			,name
        			,content
        			,price
        			,reg_date
        			,save_name
        			,file_path
        			,user_no
			from goods
    	]]>
		
	</select>

</mapper>